{
  "name": "c",
  "version": "0.1.0",
  "private": true,
  "main": "src/electron-main.js",
  "scripts": {
    "dev": "vite --port 9999",
    "build": "vite build",
    "serve": "vite preview --port 9999",
    "start": "electron .",
    "dist": "vite build && electron-builder",
    "ttt": "electron-builder"
  },
  "devDependencies": {
    "@vicons/carbon": "^0.13.0",
    "@vicons/fluent": "^0.13.0",
    "@vicons/tabler": "^0.13.0",
    "@vicons/utils": "^0.1.4",
    "@vitejs/plugin-vue": "^5.2.0",
    "electron": "^33.3.1",
    "electron-builder": "^25.1.8",
    "naive-ui": "^2.41.0",
    "prettier": "^3.4.2",
    "sass": "^1.83.1",
    "style-loader": "^4.0.0",
    "typescript": "~5.7.2",
    "vfonts": "^0.0.3",
    "vite": "^6.0.6",
    "vue": "^3.5.13",
    "vue-class-component": "^8.0.0-0",
    "vue-router": "^4.5.0",
    "vuex": "^4.1.0"
  },
  "build": {
    "appId": "C",
    "productName": "C",
    "icon": "public/favicon.ico",
    "copyright": "C Â© 2025",
    "compression": "maximum",
    "afterSign": "src/electron-utils/afterSign.js",
    "asar": true,
    "win": {
      "icon": "public/favicon.ico",
      "target": "nsis",
      "legalTrademarks": "C"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": false
    },
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "src/electron-main.js",
      "src/preload.js",
      "src/electron-utils/*"
    ],
    "extraResources": []
  },
  "dependencies": {
    "asar": "^3.2.0",
    "asarmor": "^3.0.1",
    "javascript-obfuscator": "^4.1.1",
    "vite-plugin-electron": "^0.29.0"
  }
}
